<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.googlemeet">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoogleMeet">
        <activity
            android:name=".Views.FeedBackActivity"
            android:exported="true" />

        <activity android:name=".Views.DashboardActivity"
            android:exported="true" />
        <activity android:name=".Views.SignInActivity"
            android:exported="true" />

        <activity android:name=".Views.NewMeetActivity"
            android:exported="true"/>

        <activity
            android:name=".Views.JoinGoogleMeetActivity"
            android:exported="true"
            android:label="@string/title_activity_join"
            android:theme="@style/Theme.GoogleMeet.NoActionBar" />
        <activity
            android:name="Views.MainScreen"
            android:exported="true" />
        <activity android:name=".Views.splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>